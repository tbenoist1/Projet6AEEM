<form method="post" action="{{ path('SupprimerMairieId', {'id': mairie.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cet élément ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ mairie.id) }}">
    <button class="btn btn-primary">Effacer</button>
</form>
